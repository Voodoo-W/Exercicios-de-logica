algoritmo "EXERCICO_09"
var

   VOLUME, RAIO : REAL

inicio

      ESCREVA ("QUAL O VALOR DO RAIO? ")
      LEIA (RAIO)
      
      //PI 3.14159
      
      VOLUME <- 4 / 3 * (3.14159 * RAIO ^ 3)

      // SAIDA DE DADOS

      ESCREVAL ("VOLUME = ", VOLUME)
      
fimalgoritmo
